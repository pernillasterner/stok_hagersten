  border: 1px solid #9b9b9b;
  box-shadow: 0 2px 3px #e3e3e3 inset;
}

.ninja-forms-form textarea {
  max-height: 100px;
}

.ninja-forms-form label {
  font-weight: normal !important;
}

.field-wrap, #ninja_forms_required_items {
  position: relative;
}

.field-wrap.pie-wrap {
  float: left;
  clear: both;
}

.field-wrap .ninja-forms-field-error {
  display: none;
}

.field-wrap.ninja-forms-error .ninja-forms-field-error {
  display: block;
}

.ninja-forms-field-error {
    text-align: center;
    right: 0;
    position: absolute;
    background: #FFF;
    width: 180px;
    top: -20px;
    font-size: 12px;
    border: 1px solid #CCC;
    padding: 5px;
}

.ninja-forms-field-error:after {
  content: "";
  background: #FFF;
  border-left: 1px solid #CCC;
  border-bottom: 1px solid #CCC;
  position: absolute;
  width: 10px;
  height: 10px;
  bottom: -6px;
  left: 50%;
  margin-left: -5px;
  transform: rotate(-45deg);
}


