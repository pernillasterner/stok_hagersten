/* start page carousel */

.carousel-mobile{
    display:none;
}

#carousel{
  -moz-border-radius:10px;
  -webkit-border-radius:10px;
  border-radius:10px;
  width:700px;
  height:327px;
  position:relative;
  overflow:hidden;
  background:url(bilder/loading.gif) center center no-repeat;
}
  #carousel img, .carousel-mobile img{
    width:700px;
    height:327px;
    -moz-border-radius:10px;
    -webkit-border-radius:10px;
    border-radius:15px;
  }

  #carousel .item{
    cursor:pointer;
    height:327px;
  }

  /* Hide the content of the carousel to prevent jumpy loading in IE 7 & 8. This is laters shown with jQuery */
  #browsable{
    display:none;
    width:700px;
    height:327px;
  }

  .carousel-text{
    width:652px;
    height:91px;
    background:url(bilder/carousel-text-bg.png);
    position:absolute;
    left:0;
    bottom:0;
    padding:0 24px;
  }

    .carousel-text h2{
      margin:9px 0 0 0;
    }

    .carousel-text h2 a{
      color:#a91e2b;
      font-size:18px;
    }

    .carousel-text p{
      color:#353535;
      font-size:13px;
      line-height:17px;
      max-width:550px;
    }

      .carousel-text p a{
        font-weight:bold;
        color:#353535;
        margin-left:5px;
      }

  .navi {
      height: 20px;
      position: absolute;
      bottom: 22px;
      width: 86px;
      right: 10px;
      z-index: 9;

      text-align: right;
}

    .navi a{
        background: url(bilder/navigator.png) no-repeat;
