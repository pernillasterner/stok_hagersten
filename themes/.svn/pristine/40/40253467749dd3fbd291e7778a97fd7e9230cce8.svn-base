/* NAVIGATION */

#menu-mainmenu {
    margin-left: 0;
    width: 100%;
    background: #dedede;
    /*background: rgb(243,242,240); /* Old browsers */*/
                /* IE9 SVG, needs conditional override of 'filter' to 'none' */
    background: url(data:image/svg+xml;base64,);
    background: -moz-linear-gradient(top,  rgba(243,242,240,1) 0%, rgba(227,227,224,1) 100%); /* FF3.6+ */
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(243,242,240,1)), color-stop(100%,rgba(227,227,224,1))); /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(top,  rgba(243,242,240,1) 0%,rgba(227,227,224,1) 100%); /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(top,  rgba(243,242,240,1) 0%,rgba(227,227,224,1) 100%); /* Opera 11.10+ */
    background: -ms-linear-gradient(top,  rgba(243,242,240,1) 0%,rgba(227,227,224,1) 100%); /* IE10+ */
    background: linear-gradient(top,  rgba(243,242,240,1) 0%,rgba(227,227,224,1) 100%); /* W3C */
    background: linear-gradient(#f3f2f0, #e3e3e0);
        -pie-background: linear-gradient(#f3f2f0, #e3e3e0);
    height: 41px;
}

.main-menu ul {
  list-style: none;
  margin: 23px 5px 0 12px;
  padding: 0;
  width: 100%;
}

  .main-menu.pie ul li{
    border-right: 1px solid #ddd;
    border-left: 1px solid #eee;
    display:block;
    float:left;
    padding:10px 20px;
    z-index: 200;
  }

    .main-menu.pie ul li.current_page_item, .main-menu ul li.current_page_parent{
      /* background:url(bilder/main-nav-active.png) repeat-x; */
      border:0;
      padding:10px 20px;
      background: #9a8d69;
      background: -webkit-gradient(linear, 0 0, 0 bottom, from(#b5a98e), to(#9a8d69));
      background: -webkit-linear-gradient(#b5a98e, #9a8d69);
      background: -moz-linear-gradient(#b5a98e, #9a8d69);
      background: -ms-linear-gradient(#b5a98e, #9a8d69);
      background: -o-linear-gradient(#b5a98e, #9a8d69);
      background: linear-gradient(#b5a98e, #9a8d69);
      -pie-background: linear-gradient(#b5a98e, #9a8d69);
    }

      .main-menu ul li.current_page_item a, .main-menu ul li.current_page_item a:hover, .main-menu ul li.current_page_parent a, .main-menu ul li.current_page_parent a:hover{
        color:#fff;
      }

    .main-menu ul li a{
      color:#000;
      font-size:14px;
      font-weight: bold;
    }

      .main-menu ul li a:hover{
        color:#A91E2B;
      }

.sub-menu{
  border:1px solid #d6cda9;
  -moz-border-radius:10px;
  -webkit-border-radius:10px;
  border-radius:10px;
  margin:0 0 10px 0;
overflow:hidden;
}

    .sub-menu.before-share{
  margin:0 0 20px 0;
    }

  .sub-menu ul{
    margin:0;
    padding:0;
    list-style:none;
  }



    .sub-menu + li{
      border:0;
    }

    .sub-menu li + li{
      border-top: 1px solid #D5CDAA;
    }

    .sub-menu ul li a{
      display: inline-block;
      font-size: 14px;
      font-weight: bold;
      margin: 7px 10px;
      color: #353535;
    }

    .sub-menu ul li a:hover{
      color:#353535;
    }

    .sub-menu ul li ul{
      background:#F7F5EE;
      border-top: 1px solid #D5CDAA;
    }

      .sub-menu ul li ul li{
        margin:0 10px;
      }

      .sub-menu ul li ul li a, .sub-menu ul li.sel ul li a{
        font-size:12px;
        font-weight:normal;
        color:#353535;
      }

      .sub-menu ul li ul li a:hover{
        color:#a91e2b;
      }


      .sub-menu li.sel a, .sub-menu li.sel li.sel a{
        color:#A91E2B;
        font-weight:bold;
      }

                        .category .sub-menu li.sel a, .category .sub-menu li.sel li.sel a{
        color:#353535;
      }

                        .sub-menu li.current-cat a, .sub-menu li.current-cat li.current-cat a{
        color:#A91E2B;
        font-weight:bold;
      }





